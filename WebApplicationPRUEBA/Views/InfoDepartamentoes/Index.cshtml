@model IEnumerable<WebApplicationPRUEBA.Models.Departamento>

@{
    ViewBag.Title = "Index";
}

<h2>Departamentos</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.codigoDepartamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idManager)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.codigoDepartamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idManager)
        </td>
       
    </tr>
}

</table>
